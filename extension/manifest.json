{
    "manifest_version": 3,
    "name": "GMaps in Search",
    "description": "Local test now",
    "version": "2.0",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
        {
            "matches": ["https://www.google.com/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Redirect",
        "default_icon": {
          "16": "icon.png",
          "48": "icon.png",
          "128": "icon.png"
        }
      },
      "default_locale": "en"
}